{
  "canisters": {
    "reversi": {
      "main": "src/reversi/main.mo",
      "type": "motoko"
    },
    "reversi_assets": {
      "dependencies": [
        "reversi"
      ],
      "frontend": {
        "entrypoint": "src/reversi_assets/public/index.html"
      },
      "source": [
        "src/reversi_assets/assets",
        "dist/reversi_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.0",
  "networks": {
    "local": {
      "bind": "0.0.0.0:8000",
      "type": "ephemeral"
    },
    "sodium": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    },
    "mainnet": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
